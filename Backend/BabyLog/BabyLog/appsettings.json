{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 9223372036854775807,
      "MaxResponseBufferSize": 104857600,
      "MaxRequestBufferSize": 104857600,
      "KeepAliveTimeout": "00:10:00",
      "RequestHeadersTimeout": "00:05:00"
    },
    "DisableStringReuse": true,
    "AllowSynchronousIO": true
  }
}
