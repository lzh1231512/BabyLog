{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "http://localhost:5099",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_MaxRequestBodySize": "1073741824",
        "ASPNETCORE_HTTP_UPLOAD_LIMIT": "1073741824",
        "ASPNETCORE_KeepAliveTimeout": "600",
        "ASPNETCORE_RequestHeadersTimeout": "300",
        "ASPNETCORE_ENABLE_REQUEST_BODY_BUFFERING": "false",
        "ASPNETCORE_DISABLE_REQUEST_LIMITS": "true"
      }
    }
  }
}
